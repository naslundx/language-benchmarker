{
  "helloworld":
  {
    "build": ["mkdir -p bin", "g++ helloworld.cpp -o bin/helloworld -O3"],
    "run": "./bin/helloworld",
    "clean": "rm -rf bin"
  },
  "primes":
  {
    "build": ["mkdir -p bin", "g++ primes.cpp -o bin/primes -O3"],
    "run": "./bin/primes",
    "clean": "rm -rf bin"
  },
  "binarytree":
  {
    "build": ["mkdir -p bin", "g++ binarytree.cpp -o bin/binarytree -O3 --std=c++11"],
    "run": "./bin/binarytree",
    "clean": "rm -rf bin"
  }
}
