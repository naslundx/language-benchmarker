{
  "helloworld":
  {
    "build": ["ghc helloworld.hs -o helloworld", "mkdir -p bin", "mv helloworld bin/helloworld", "mv helloworld.hi bin/helloworld.hi", "mv helloworld.o bin/helloworld.o"],
    "run": "./bin/helloworld",
    "clean": "rm -rf bin"
  },
  "primes":
  {
    "build": ["ghc primes.hs -o primes", "mkdir -p bin", "mv primes bin/primes", "mv primes.hi bin/primes.hi", "mv primes.o bin/primes.o"],
    "run": "./bin/primes",
    "clean": "rm -rf bin"
  }
}
